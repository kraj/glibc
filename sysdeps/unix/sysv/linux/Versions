  GLIBC_2.32 {
  }
  GLIBC_2.34 {
%ifdef TIME64_NON_DEFAULT
    # 64 bit time_t support
    # io
    __ppoll64;
    __utime64;
    # misc
    __futimes64;
    __futimesat64;
    __lutimes64;
    __select64;
    __utimes64;
